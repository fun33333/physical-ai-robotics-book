# Gemini API Keys (3 keys for rotation)
GEMINI_API_KEY_1=your_gemini_api_key_1_here
GEMINI_API_KEY_2=your_gemini_api_key_2_here
GEMINI_API_KEY_3=your_gemini_api_key_3_here

# Qdrant Vector Database
QDRANT_URL=https://your-qdrant-cluster.qdrant.io
QDRANT_API_KEY=your_qdrant_api_key_here

# Neon Postgres Database
DATABASE_URL=postgresql://user:password@host.neon.tech/database_name

# Frontend CORS Origin
CORS_ORIGIN=http://localhost:3000,https://your-docusaurus-site.com

# Environment Mode
ENVIRONMENT=development

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8000

# Logging
LOG_LEVEL=INFO

# Vector Database Collection Name
QDRANT_COLLECTION_NAME=textbook_chapters
